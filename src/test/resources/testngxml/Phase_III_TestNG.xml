			<class
				name="com.qait.automation.TestAssociationFinancialProfessional.TestUserCancelCTP_Exam" />
		</classes>
	</test>

	<test name="Linking individual to a new conmpany" parallel="false"
		preserve-order="true">
		<classes>
			<class
				name="com.qait.automation.TestAssociationFinancialProfessional.TestUserLinkIndividualToNewCompany" />
		</classes>
