	<test name="CS SOP Create Ceu-Reporting InHouse" parallel="false"
		preserve-order="true">
		<classes>
			<class
				name="com.qait.automation.TestAssociationFinancialProfessional.TestUserCreateCEU_ReportingInHouse" />
		</classes>
	</test>
	
		<test name="CS SOP Renew Membership eWeb" parallel="false"
		preserve-order="true">
		<classes>
			<class
				name="com.qait.automation.TestAssociationFinancialProfessional.TestUserRenewMembershipEweb" />
		</classes>
	</test>
</suite>
