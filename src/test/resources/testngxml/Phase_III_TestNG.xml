<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name=" AFP Automated Test Suite Phase-III" parallel="classes"
	thread-count="1" preserve-order="true">

	<listeners>
		<listener class-name="com.qait.automation.report.CustomReport" />
	</listeners>

	<test name="CS SOP Discount Product" parallel="false"
		preserve-order="true">
