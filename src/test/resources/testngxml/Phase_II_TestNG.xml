<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name=" AFP Automated Test Suite Phase-II" parallel="classes"
	thread-count="1" preserve-order="true">

	<listeners>
		<listener class-name="com.qait.automation.report.CustomReport" />
	</listeners>

	<test name="CS SOP Discount Webinar" parallel="false" preserve-order="true">
		<classes>
			<class
				name="com.qait.automation.TestAssociationFinancialProfessional.TestUserCreateDiscountForWebinar" />
		</classes>
	</test>

	<test name="CS SOP - Order Fullfillment" parallel="false" preserve-order="true">
		<classes>
			<class
				name="com.qait.automation.TestAssociationFinancialProfessional.TestUserPerformsOrderFulfilment" />
		</classes>
	</test>
	
	<test name="LockBox check paying for multiple items" parallel="false" preserve-order="true">
		<classes>
			<class
				name="com.qait.automation.TestAssociationFinancialProfessional.TestLockBoxCheckPayingForMultipleItems" />
		</classes>
	</test>
	
	
</suite>
